{
  "manifest_version": 3,
  "name": "Kueez CMS Scraper",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://cms.kueez.net/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "public/vite.svg"
  },
  "content_scripts": [
    {
      "matches": ["https://cms.kueez.net/*"],
      "js": ["src/content.js"]
    }
  ]
}
